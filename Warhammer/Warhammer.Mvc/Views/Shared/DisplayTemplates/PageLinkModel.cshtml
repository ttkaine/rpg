@using Warhammer.Mvc.Models
@model Warhammer.Core.Models.PageLinkModel
<tr>
    <td>
        @Html.Partial("PageImage", new ImageViewModel(Model))
    </td>
    <td>
        @Model.FullName
        @if (Model.ShortName != Model.FullName)
        {<span class="note">(@Model.ShortName)</span>}
    </td>
</tr>


@model Warhammer.Mvc.Models.UserSettingsSectionViewModel

<div id="setting-@Model.SettingTitle" class="setting-section">
    <h3>@Model.SettingTitle</h3>

    <table class="table table-striped">
        <thead>
        <tr>
            <th>Setting</th>
            <th>Description</th>
            <th>Status</th>
        </tr>
        </thead>
        <tbody>
        @Html.DisplayFor(m => m.Settings)
        </tbody>

    </table>
</div>
